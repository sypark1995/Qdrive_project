<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no" />

    	<title></title>
    
		<link rel="stylesheet" href="../libs/css/jquery.mobile-1.1.0.min.css" type="text/css" />
		<link rel="stylesheet" href="../libs/css/common.css" type="text/css" />
		<link rel="stylesheet" href="../libs/css/mobile_app.css" type="text/css" />	
	
		<script type="text/javascript" charset="utf-8" src="../cordova.js"></script>
	 	<script type="text/javascript" charset="utf-8" src="../libs/js/jquery-1.7.2.js"></script>
		<script type="text/javascript" charset="utf-8" src="../libs/js/jquery.mobile-1.1.0.js"></script>
		<script type="text/javascript" charset="utf-8" src="../libs/js/giosis_mobile_util.js"></script>
		<script type="text/javascript" charset="utf-8" src="../libs/js/loading_dialog.js"></script>					
		<script type="text/javascript" charset="utf-8" src="../libs/js/common.js"></script>
		<script type="text/javascript" charset="utf-8" src="./scripts/noticeDetail.js"></script>

	</head>
	<body onload="init();">
		<!-- WRAP -->
		<div data-role="page" id="wrap" class="settings">
			<!-- HEADER -->
			<div class="hd" data-role="header" data-theme="b" data-position="fixed" >
		        <h1>Notice</h1>           
				<!--<a href="" id="btnBack" class="btn_back">Back</a>-->
				<div class=btn_area_left>
					<a id="btnBack" data-ajax="false" class="ui-link" href=""  >Back</a>
				</div>
		    </div><!-- //HEADER --> 	         
		
			<!-- CONTENTS -->
			<div class="content" >
				<div class="section_index" style="min-height:380px;">
		            <div class="notice_con">
		                <div class="date" id="divDate"></div>
		                <div class="contents">
		                    <h4 id="hTitle"></h4>
		                    <div class="article" id="divContents">
		                	</div>
		                </div>
		                <div class="btn_paging">
		                    <a href="" class="btn_basic" id="btnBackToList"><span>Back to list</span></a>
		                    <p>
		                        <a href="" class="btn_basic pre" id="btnPrev"><span class="icon_pre"></span>Prev</a>
		                        <a href="" class="btn_basic nxt" id="btnNext">Next<span class="icon_nxt"></span></a>
		                    </p>
		                </div>
		            </div>		    
		        </div><!-- //SECTION INDEX -->
			</div><!-- //CONTENTS -->
			
		    <!-- ALERT : NETWORK DISCONNECTED -->
		    <div class="qlps_alert reload" id="qlps_alert_reload" style="top:70px; left:5px; display:none;">
		        <div class="inner">
		            <p>Network disconnected.</p>
		            <a href="" id="btnReload"><span class="icon_reload"></span></a>
		        </div>   
		    </div>
		    <!-- // ALERT : NETWORK DISCONNECTED -->
		    			
		</div><!-- WRAP -->
	</body>
</html>