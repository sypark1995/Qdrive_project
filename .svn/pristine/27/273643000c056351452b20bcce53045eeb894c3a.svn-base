package gmkt.inc.android.common.network;

import java.io.File;
import java.util.ArrayList;
import java.util.HashMap;

/**
 * 
 * MobileAPIService에 요청할 데이터 클래스 
 * 
 * @author wontae
 *
 */
public class GMKT_MobileAPIServiceRequestData {

	private String 								targetURL;
	private String 								methodName;
	
	private HashMap<String, String> 			params;
	private HashMap<String, File>               fileParams;
	
	private String								documentNodeName;
	private String 								rootNodeName; 
	private ArrayList<String>					rootNodeNameList;
	
	private int									xmlParserType;
	
	private String                              method;
	
	/**
	 * @return the targetURL
	 */
	public String getTargetURL() {
		return targetURL;
	}

	/**
	 * @param targetURL the targetURL to set
	 */
	public void setTargetURL(String targetURL) {
		this.targetURL = targetURL;
	}

	/**
	 * @return the methodName
	 */
	public String getMethodName() {
		return methodName;
	}

	/**
	 * @param methodName the methodName to set
	 */
	public void setMethodName(String methodName) {
		this.methodName = methodName;
	}

	/**
	 * @return the params
	 */
	public HashMap<String, String> getParams() {
		return params;
	}

	/**
	 * @param params the params to set
	 */
	public void setParams(HashMap<String, String> params) {
		this.params = params;
	}

	/**
	 * @return the documentNodeName
	 */
	public String getDocumentNodeName() {
		return documentNodeName;
	}

	/**
	 * @param documentNodeName the documentNodeName to set
	 */
	public void setDocumentNodeName(String documentNodeName) {
		this.documentNodeName = documentNodeName;
	}

	/**
	 * @return the rootNodeName
	 */
	public String getRootNodeName() {
		return rootNodeName;
	}

	/**
	 * @param rootNodeName the rootNodeName to set
	 */
	public void setRootNodeName(String rootNodeName) {
		this.rootNodeName = rootNodeName;
	}

	/**
	 * @return the xmlParserType
	 */
	public int getXmlParserType() {
		return xmlParserType;
	}

	/**
	 * @param xmlParserType the xmlParserType to set
	 */
	public void setXmlParserType(int xmlParserType) {
		this.xmlParserType = xmlParserType;
	}

    /**
     * @return the fileParams
     */
    public HashMap<String, File> getFileParams() {
        return fileParams;
    }

    /**
     * @param fileParams the fileParams to set
     */
    public void setFileParams(HashMap<String, File> fileParams) {
        this.fileParams = fileParams;
    }

    /**
     * @return the method
     */
    public String getMethod() {
        return method;
    }

    /**
     * @param method the method to set
     */
    public void setMethod(String method) {
        this.method = method;
    }

	public ArrayList<String> getRootNodeNameList() {
		return rootNodeNameList;
	}

	public void setRootNodeNameList(ArrayList<String> rootNodeNameList) {
		this.rootNodeNameList = rootNodeNameList;
	}
	
}
