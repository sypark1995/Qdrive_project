ui-header@charset "utf-8";
/* ----------------------------------------------------------------------
    COMMON
----------------------------------------------------------------------*/
body,input,textarea,select,button,table{font-family:Helvetica, AppleGothic, sans-serif; line-height:1.331;font-size:12px;}
*{text-shadow:none;}
#wrap{min-height:90% !important;}
.ellipsis{text-overflow:ellipsis; overflow:hidden; white-space:nowrap;}
.bg_white{background:#fff;}

/* ----------------------------------------------------------------------
    HEADER BLUE STYLE
----------------------------------------------------------------------*/
/* BACKGROUND */
.ui-bar-b{background: #4FB648;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#4FB648), to(#4FB648));
    background-image: -webkit-linear-gradient(#4FB648, #4FB648);
    background-image: -moz-linear-gradient(#4FB648, #4FB648);
    background-image: -ms-linear-gradient(#4FB648, #4FB648);
    background-image: -o-linear-gradient(#4FB648, #4FB648);
    background-image: linear-gradient(#4FB648, #4FB648);}
/* BUTTON */
.ui-btn-up-b{background: #4FB648;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#4FB648), to(#4FB648));
    background-image: -webkit-linear-gradient(#4FB648, #4FB648);
    background-image: -moz-linear-gradient(#4FB648, #4FB648);
    background-image: -ms-linear-gradient(#4FB648, #4FB648);
    background-image: -o-linear-gradient(#4FB648, #4FB648);
    background-image: linear-gradient(#4FB648, #4FB648);}
.ui-btn-hover-b{background: #4FB648;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#4FB648), to(#4FB648));
    background-image: -webkit-linear-gradient(#4FB648, #4FB648);
    background-image: -moz-linear-gradient(#4FB648, #4FB648);
    background-image: -ms-linear-gradient(#4FB648, #4FB648);
    background-image: -o-linear-gradient(#4FB648, #4FB648);
    background-image: linear-gradient(#4FB648, #4FB648);}
.ui-header .ui-btn .ui-btn-text{font-weight:normal;}
.ui-header .ui-btn-inner, .ui-footer .ui-btn-inner, .ui-bar .ui-btn-inner{padding:.5em 15px .5em;}
.ui-corner-bottom,.ui-corner-top{border-radius:.8em;}
/* BUTTON : RIGHT SIDE BUTTON */
.ui-header .btn_area_right{position:absolute; top:7px; right:11px; margin:0;}
.ui-header .btn_area_right a:link, .ui-header .btn_area_right a:visited{
    display:inline-block; vertical-align:middle; border-radius:.5em;font-size:13px;padding:4px 7px; box-shadow:inset 0 1px 0 #63afec; overflow:hidden;
    padding-top:8px;
    padding-bottom:8px;
    padding-left:8px;
    padding-right:8px;
    border: 0px;
    font-weight: normal;
    color: #4FB648;
    background: #fff;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#fff), to(#fff));
    background-image: -webkit-linear-gradient(#fff, #fff);
    background-image: -moz-linear-gradient(#fff, #fff);
    background-image: -ms-linear-gradient(#fff, #fff);
    background-image: -o-linear-gradient(#fff, #fff);
    background-image: linear-gradient(#fff, #fff);}
.ui-header .btn_area_right a.hover, .ui-header .btn_area_right a.active {box-shadow:0 1px 0 #6ba6d8;background: #3659a6;}
.ui-header .ta_center{text-align:center;}
.ui-header .btn_area_right a.line2{line-height:13px;  padding:2px 7px 7px; height:22px;}
/* BUTTON : Left SIDE Home BUTTON */
.ui-header .btn_area_left{position:absolute; top:7px; left:11px; margin:0;}
.ui-header .btn_area_left a:link, .ui-header .btn_area_left a:visited{
    display:inline-block; vertical-align:middle; border-radius:.5em;font-size:13px;padding:4px 7px; box-shadow:inset 0 1px 0 #63afec; overflow:hidden;
    border: 0px;
    padding-top:8px;
    padding-bottom:8px;
    padding-left:8px;
    padding-right:8px;
    font-weight: normal;
    color: #4FB648;
    background: #fff;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#fff), to(#fff));
    background-image: -webkit-linear-gradient(#fff, #fff);
    background-image: -moz-linear-gradient(#fff, #fff);
    background-image: -ms-linear-gradient(#fff, #fff);
    background-image: -o-linear-gradient(#fff, #fff);
    background-image: linear-gradient(#fff, #fff);}
.ui-header .btn_area_left a.hover, .ui-header .btn_area_left a.active {box-shadow:0 1px 0 #6ba6d8;background: #fff;}
.ui-header .ta_center{text-align:center;}
.ui-header .btn_area_left a.line2{line-height:13px;  padding:2px 7px 7px; height:22px;}
/* BUTTON : LEFT SIDE BACK BUTTON */
.ui-header .ui-btn-left{top:7px;}
.ui-header .btn_back{width:68px;height:31px;padding:0 !important;background-size:59px auto;
    background:url(../image/app/ico_back.png) 0 0 no-repeat;}    
.ui-header .btn_back.hover, .ui-header .btn_back.active{background-image:url(../image/app/ico_back_hover.png)}
.ui-header .btn_back, .ui-header .btn_back .ui-btn-inner{box-shadow:none;border-radius:0;border:none;text-shadow:none;}  
.ui-header .btn_back .ui-btn-inner{padding-left:10px;}

.ui-header .btn_home{width:68px;height:27px;padding:0 !important;background-size:59px auto; background: #207dc9; background-image: -webkit-gradient(linear, left top, left bottom, from(#2e92e3), to(#156cb5));  }
/**.ui-header .btn_home.hover, .ui-header .btn_back.active{background-image:url(../image/app/ico_back_hover.png)}*/
.ui-header .btn_home{    box-shadow: inset 0 1px 0 #63afec;border-radius: .5em !important;text-shadow:none; }
/**.ui-header .btn_home .ui-btn-inner{padding-left:10px;}*/

.ui-header .btn_srch{width:42px;height:32px;
    background:url(../image/app/ico_search.png) no-repeat;background-size:84px;}
.ui-header .btn_srch:active{background-position:-42px 0;}
.ui-header .btn_srch, .ui-header .btn_srch .ui-btn-inner{box-shadow:none;border-radius:0;border:none;text-shadow:none;}  

/* H1 : TITLE */
.ui-header .ui-title, .ui-footer .ui-title{font-size:28px;margin:11px 70px 15px;}
.ui-header.hd {height:43px;border-top:none;}
.ui-header.hd h1{height:34px; overflow:hidden;font-size:19px; margin:10px 5px 0;}
.header_fixed{position:fixed !important;z-index:1000;width:100%;}

/* ----------------------------------------------------------------------
    CONTENT STYLE
----------------------------------------------------------------------*/
.content{position:relative;}
.content_fixed{margin-top:128px;}
.content .section{min-height:180px;}

.h1_comment{height:18px;background-color:#ebebeb; color:#345497; text-shadow:none; padding:0 0 0 10px; font-size:11px; margin:0; font-weight:normal; position:relative;line-height:18px;}
.h1_comment strong{font-size:13px;font-weight:normal;}
.h1_comment .customer_id{position:absolute;top:2px;right:6px;color:#747474;font-weight:normal;font-size:12px;}
.h1_comment .customer_id.blue{color:#869ac8;}
.h1_comment .customer_id .icon{
	display:inline-block; width:12px; height:12px; margin:1px 5px 5px 0; vertical-align:middle;
	background:url(../image/app/ico_mobile.png) 0 0 no-repeat;background-size:340px auto;}

.lists li{position:relative;padding:7px 15px 6px;border-top:1px solid #fff;border-bottom:1px solid #c7c7c7;background-color:#f2f2f2;font-size:16px;color:#5d5d5d;}
.lists li span.con{position:absolute;top:10px;right:10px;font-size:13px;color:#2358c2;}
.notice_y{padding:2px 8px;background-color:#edd75b;border:1px solid #ddc541;border-radius:5px;font-size:11px;font-weight:bold;color:#333;}
.notice_yg{margin:6px 0;padding:6px 12px;background-color:#edd75b;border:1px solid #a59443;border-radius:7px;font-size:11px;font-weight:bold;color:#333;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#e8cf40), to(#f4eab6));
    background-image: -webkit-linear-gradient(#e8cf40, #f4eab6);
    background-image: -moz-linear-gradient(#e8cf40, #f4eab6);
    background-image: -ms-linear-gradient(#e8cf40, #f4eab6);
    background-image: -o-linear-gradient(#e8cf40, #f4eab6);
    background-image: linear-gradient(#e8cf40, #f4eab6);}
.notice_n{padding:2px 10px;background-color:#5e6980;border-radius:10px;font-size:11px;font-weight:bold;color:#fff;}
.notice_m{padding:2px 10px;background-color:#dddddd;border-radius:10px; font-size:12px;font-weight:bold;color:#333;}
.notice_memo {padding:2px 10px;background-color:#FFFF6B;border-radius:10px;font-size:11px;color:black; }

/* ----------------------------------------------------------------------
    CONTENT COMMON : ICONS
----------------------------------------------------------------------*/

    
/* SLIDER:SWICH */
.slider_swc{position:relative;display:block;width:68px;height:25px;border:1px solid #c3c3c3;border-color:#0e3d8e #3063b2 #4e84d2;border-radius:3px;
	background:#337af2;
	background:-moz-linear-gradient(top, #337af2 0%, #357ff8 50%, #4e91f3 51%, #70a8fb 100%);
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#337af2), color-stop(50%,#357ff8), color-stop(51%,#4e91f3), color-stop(100%,#70a8fb));
	background:-webkit-linear-gradient(top, #337af2 0%,#357ff8 50%,#4e91f3 51%,#70a8fb 100%);
	background:-o-linear-gradient(top, #337af2 0%,#357ff8 50%,#4e91f3 51%,#70a8fb 100%);
	background:-ms-linear-gradient(top, #337af2 0%,#357ff8 50%,#4e91f3 51%,#70a8fb 100%);
	background:linear-gradient(to bottom, #337af2 0%,#357ff8 50%,#4e91f3 51%,#70a8fb 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#337af2', endColorstr='#70a8fb',GradientType=0);}
.slider_swc .swc{float:left;display:block;text-align:center;width:50%;height:25px;line-height:25px;overflow:hidden;cursor:pointer;top:0;border-width:0;white-space:nowrap;font-size:12px;color:#fff;text-shadow:0 -1px 0 #213c6a;font-weight:bold;box-shadow:inet 0 1px 1px #2968d7}
.slider_swc .swc:last-child{float:right}
.slider_swc .swc.inert{position:absolute;top:0;margin:-1px;overflow:hidden;border-radius:3px;border:1px solid #000;border-color:#919191 #8e9196 #999 #9a9a9a;color:#7f7f7f;text-shadow:0 0 0 #fff;box-shadow:-1px 0 1px rgba(0,0,0,.3);
	background:#c7c7c7;
	background:-moz-linear-gradient(top, #c7c7c7 0%, #acacac 100%);
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#c7c7c7), color-stop(100%,#acacac));
	background:-webkit-linear-gradient(top, #c7c7c7 0%,#acacac 100%);
	background:-o-linear-gradient(top, #c7c7c7 0%,#acacac 100%);
	background:-ms-linear-gradient(top, #c7c7c7 0%,#acacac 100%);
	background:linear-gradient(to bottom, #c7c7c7 0%,#acacac 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#c7c7c7', endColorstr='#acacac',GradientType=0);}
.slider_swc .swc.inert.on{left:0}
.slider_swc .swc.inert.off{right:0}

