/* BT_BIG */
.bt_big{position:relative;display:block;margin-top:6px;padding:10px 15px;border-radius:8px;border:1px solid #ababab;font-size:14px;font-weight:bold;
    background-image: #eaeaea;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#fefefe), to(#dfdfdf));
    background-image: -webkit-linear-gradient(#fefefe, #dfdfdf);
    background-image: -moz-linear-gradient(#fefefe, #dfdfdf);
    background-image: -ms-linear-gradient(#fefefe, #dfdfdf);
    background-image: -o-linear-gradient(#fefefe, #dfdfdf);
    background-image: linear-gradient(#fefefe, #dfdfdf);}
.bt_big.hover, .bt_big.active{background:#555;color:#fff !important;}
.bt_big.hover *, .bt_big.active *{color:#fff !important;}
.bt_big .r_arrow{position:absolute;top:30%;right:10px;width:12px;height:18px;
    background:url(../image/app/ico_mobile.png) -228px -17px no-repeat;background-size:360px auto}
.bt_big.hover .r_arrow, .bt_big.active .r_arrow{background-position:-317px -17px !important;}
.bt_big.rv{background-image: #eaeaea;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#dfdfdf), to(#fefefe));
    background-image: -webkit-linear-gradient(#dfdfdf, #fefefe);
    background-image: -moz-linear-gradient(#dfdfdf, #fefefe);
    background-image: -ms-linear-gradient(#dfdfdf, #fefefe);
    background-image: -o-linear-gradient(#dfdfdf, #fefefe);
    background-image: linear-gradient(#dfdfdf, #fefefe);}
/* BT_BIG : GREEN*/
.bt_big.green{color:#0b5f71;border:1px solid #72b9c8;
	background-image: #aad9e4;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#deeff3), to(#99d2df));
    background-image: -webkit-linear-gradient(#deeff3, #99d2df);
    background-image: -ms-linear-gradient(#deeff3, #99d2df);
    background-image: -o-linear-gradient(#deeff3, #99d2df);
    background-image: linear-gradient(#deeff3, #99d2df);}
.bt_big.green .r_arrow{background-position:-317px -17px}
/* BT_BIG : BLUE */
.bt_big.blue .r_arrow{background-position:-226px -17px}
/* BT_BIG : GRAY*/
.bt_big.gray{color:#5d5d5d;}
.bt_big.dark_gray{color:#4b4b4b;}
.bt_big.gray .r_arrow{background-position:-245px -17px}
/* BT_BIG > FONT COLOR */
.bt_big i{font-style:normal}
.bt_big i.blue{color:#2173c5}
.bt_big i.red{color:#c35050}



/* ----------------------------------------------------------------------
    CONTENT COMMON : ALERT
----------------------------------------------------------------------*/
.qlps_pop{margin-top:5px;z-index:1;position:absolute;top:52%;left:5%;right:5%; border:1px solid #c1c1c1;background-color:#fff;border-radius:10px;box-shadow:1px 1px 0 #f5f5f5;}
.qlps_pop .inner{margin:10px;font-size:15px;text-shadow:1px 1px 1px #fff;text-align:left;} 

.qlps_pop .inner dl{position:relative;font-weight:normal;border-bottom:1px solid #cfcfcf;}
.qlps_pop .inner dt{color:#0066cc;font-size:16px;text-shadow:1px 1px 1px #fff;}
.qlps_pop .inner dd{position:absolute;top:0;color:#3d4e8d;font-size:15px;}

/* ----------------------------------------------------------------------
    CONTENT COMMON : ALERT
----------------------------------------------------------------------*/
.qlps_alert{z-index:100;position:absolute;top:20%;left:10%;right:10%;background:#000;border-radius:25px;background-color: rgba(0,0,0,0.82); }
.qlps_alert .inner{margin:20px;color:#fff;text-align:center;font-size:13px;}
.qlps_alert .inner2{margin:10px;color:#fff;text-align:left;font-size:13px;}
.qlps_alert .inner3{margin:5px;color:#fff;text-align:center;font-size:13px;}
.qlps_alert .inner4{margin:10px;color:#fff;text-align:left;font-size:13px;}
.qlps_alert .inner p{margin:15px 0;}
.qlps_alert .inner p.single{margin:40px 0;}
.qlps_alert .inner p em{font-size:16px;}
.qlps_alert .inner .progressbar{overflow:hidden;height:20px;padding:1px;border:1px solid #000;border-radius:13px;text-align:left;
    background-image: #1a1a1a;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#141414), to(#222222));
    background-image: -webkit-linear-gradient(#141414, #222222);
    background-image: -moz-linear-gradient(#141414, #222222);
    background-image: -ms-linear-gradient(#141414, #222222);
